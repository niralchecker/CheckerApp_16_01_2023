        for (int i = 0; i < allLoops.size(); i++) {
            if (loopName != null
                    && allLoops.get(i).getCurrentLoopName() != null
                    && allLoops.get(i).getCurrentLoopName().equals(loopName)) {
                return allLoops.get(i);
            }
        }
        return null;
    }

